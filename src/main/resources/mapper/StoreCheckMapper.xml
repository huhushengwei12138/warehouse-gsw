        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.wanho.mapper.StoreCheckMapper">

    <resultMap id="BaseResultMap" type="net.wanho.po.StoreCheck">
